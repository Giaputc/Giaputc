#include <iostream>
using namespace std;

int n, k, ok;
int a[100];

void khoitao() {
    for (int i = 1; i <= k; i++) {
        a[i] = i;
    }
}

void sinh() {
    int i=k;
    while (i>=1&&a[i]==n-k+i)
    {
        --i;
    }if(i==0){
        ok=0;
    }else{
        a[i]++;
        for(int j=i+1;j<=k;++j){
            a[j]=a[j-1]+1;
        }
    }
    
}

int main() {
    cin >> n >> k;
    ok = 1;
    khoitao();
    while (ok) {
         
            for (int i = 1; i <= n; i++) {
                cout << a[i];
            }
        cout << endl;
        sinh();
    }
    return 0;
}
