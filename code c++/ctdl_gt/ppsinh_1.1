#include <iostream>
using namespace std;

int n, k, ok;
int a[100];

void khoitao() {
    for (int i = 1; i <= n; i++) {
        a[i] = 0;
    }
}

void sinh() {
    int i = n;
    while (i >= 1 && a[i] == 1) {
        a[i] = 0;
        i--;
    }
    if (i == 0) ok = 0;
    else a[i] = 1;
}

bool check() {
    int cn = 0;
    for (int i = 1; i <= n; i++) {
        if (a[i] == 1) cn++;
    }
    return cn == k;
}

int main() {
    cin >> n >> k;
    ok = 1;
    khoitao();
    while (ok) {
        if (check()) {
            for (int i = 1; i <= n; i++) {
                cout << a[i];
            }
            cout << endl;
        }
        sinh();
    }
    return 0;
}
